<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Studentscourses" table="studentscourses" schema="dangkyhocphan">
        <composite-id mapped="true" class="pojo.StudentscoursesPK">
            <key-property name="studentId">
                <column name="StudentID" sql-type="varchar(10)" length="10"/>
            </key-property>
            <key-property name="classId">
                <column name="ClassID" sql-type="varchar(10)" length="10"/>
            </key-property>
            <key-property name="semesterYear">
                <column name="SemesterYear" sql-type="varchar(4)" length="4"/>
            </key-property>
            <key-property name="semesterName">
                <column name="SemesterName" sql-type="varchar(3)" length="3"/>
            </key-property>
            <key-property name="subjectId">
                <column name="SubjectID" sql-type="varchar(10)" length="10"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>