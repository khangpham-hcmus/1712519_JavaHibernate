<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Studentscourses" table="studentscourses" schema="dangkyhocphan">
        <composite-id name="studentscoursesPK" class="pojo.StudentscoursesPK">
            <key-property name="semesterYear" column="SemesterYear" length="10"
                          type="string" />
            <key-property name="semesterName" column="SemesterName"
                          length="10" type="string" />
            <key-property name="subjectId" column="SubjectID"
                          length="10" type="string" />
            <key-property name="classId" column="ClassID"
                          length="10" type="string" />
            <key-property name="studentId" column="StudentID"
                          length="10" type="string" />
        </composite-id>
    </class>
</hibernate-mapping>